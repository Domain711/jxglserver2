<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.college.dao.CollegeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.college.entity.CollegeEntity" id="collegeMap">
        <result property="id" column="id"/>
        <result property="collegenum" column="collegenum"/>
        <result property="collegename" column="collegename"/>
        <result property="createtime" column="createtime"/>
        <result property="createuser" column="createuser"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <select id="queryCollegeList" resultType="io.renren.modules.college.entity.CollegeEntity">
        select collegenum,collegename from tb_college  order by collegenum asc
    </select>
</mapper>